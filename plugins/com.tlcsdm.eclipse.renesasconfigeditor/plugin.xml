<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
    <extension point="org.eclipse.core.contenttype.contentTypes">
        <content-type
            base-type="org.eclipse.core.runtime.xml"
            file-extensions="scfg"
            id="com.tlcsdm.eclipse.renesasconfigeditor.scfg"
            name="Renesas SCFG XML File"
            priority="normal" />
        <content-type
            base-type="org.eclipse.core.runtime.xml"
            file-extensions="mtpj"
            id="com.tlcsdm.eclipse.renesasconfigeditor.mtpj"
            name="Renesas MTPJ XML File"
            priority="normal" />
        <content-type
            base-type="org.eclipse.core.runtime.xml"
            file-extensions="ewp"
            id="com.tlcsdm.eclipse.renesasconfigeditor.ewp"
            name="Renesas EWP XML File"
            priority="normal" />
    </extension>

    <extension point="org.eclipse.ui.editors">
        <editor
            id="com.tlcsdm.eclipse.renesasconfigeditor.editors.scfg"
            class="com.tlcsdm.eclipse.renesasconfigeditor.RenesasXmlEditor"
            name="SCFG XML Editor"
            icon="icons/scfg.png"
            extensions="scfg"
            default="true" />
        <editor
            id="com.tlcsdm.eclipse.renesasconfigeditor.editors.mtpj"
            class="com.tlcsdm.eclipse.renesasconfigeditor.RenesasXmlEditor"
            name="MTPJ XML Editor"
            icon="icons/mtpj.png"
            extensions="mtpj"
            default="true" />
        <editor
            id="com.tlcsdm.eclipse.renesasconfigeditor.editors.ewp"
            class="com.tlcsdm.eclipse.renesasconfigeditor.RenesasXmlEditor"
            name="EWP XML Editor"
            icon="icons/ewp.png"
            extensions="ewp"
            default="true" />
	</extension>
	
	<extension point="org.eclipse.core.contenttype.contentTypes">
            <content-type
                  base-type="org.eclipse.core.runtime.text"
                  file-extensions="h"
                  id="com.tlcsdm.eclipse.renesasconfigeditor.h"
                  name="C Header File"
                  priority="normal" />
            <content-type
                  base-type="org.eclipse.core.runtime.text"
                  file-extensions="c"
                  id="com.tlcsdm.eclipse.renesasconfigeditor.c"
                  name="C Source File"
                  priority="normal" />
            <content-type
                  base-type="org.eclipse.core.runtime.text"
                  file-extensions="html"
                  id="com.tlcsdm.eclipse.renesasconfigeditor.html"
                  name="HTML File"
                  priority="normal" />
            <content-type
                  base-type="org.eclipse.core.runtime.text"
                  file-extensions="js"
                  id="com.tlcsdm.eclipse.renesasconfigeditor.js"
                  name="JavaScript File"
                  priority="normal" />
            <content-type
                  base-type="org.eclipse.core.runtime.text"
                  file-extensions="css"
                  id="com.tlcsdm.eclipse.renesasconfigeditor.css"
                  name="CSS File"
                  priority="normal" />
      </extension>

      <extension point="org.eclipse.ui.editors">
            <editor
                  id="org.eclipse.ui.genericeditor.GenericEditor"
                  name="Generic Code Editor"
                  icon="icons/generic_editor.png"
                  class="com.tlcsdm.eclipse.renesasconfigeditor.RenesasGenericEditor"
                  default="true">
                  <contentTypeBinding contentTypeId="com.tlcsdm.eclipse.renesasconfigeditor.h" />
                  <contentTypeBinding contentTypeId="com.tlcsdm.eclipse.renesasconfigeditor.c" />
                  <contentTypeBinding contentTypeId="com.tlcsdm.eclipse.renesasconfigeditor.html" />
                  <contentTypeBinding contentTypeId="com.tlcsdm.eclipse.renesasconfigeditor.js" />
                  <contentTypeBinding contentTypeId="com.tlcsdm.eclipse.renesasconfigeditor.css" />
            </editor>
      </extension>
      
      <extension point="org.eclipse.core.contenttype.contentTypes">
            <content-type
                  base-type="org.eclipse.core.runtime.text"
                  file-extensions="inf"
                  id="com.tlcsdm.eclipse.renesasconfigeditor.inf"
                  name="INF File"
                  priority="normal" />
      </extension>

      <extension point="org.eclipse.ui.editors">
            <editor
                  class="org.eclipse.ui.editors.text.TextEditor"
                  default="true"
                  id="com.tlcsdm.eclipse.renesasconfigeditor.editors.inf"
                  name="INF Text Editor">
                  <contentTypeBinding contentTypeId="com.tlcsdm.eclipse.renesasconfigeditor.inf" />
            </editor>
      </extension>
	
	<extension point="org.eclipse.help.toc">
		<toc file="help/help_toc.xml" primary="true"/>
	</extension>

</plugin>
